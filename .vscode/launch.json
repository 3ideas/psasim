    "version": "0.2.0",
    "configurations": [
        {
            "name": "Read PSAlerts and resolve aliases from db",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-log", "output/alias_resolve.log",
                "-loglevel", "warn",
                // "-usenameservice",
                "-db", "../data_loads/load10/export_of_dataload10.db",
                "-psalerts", "ref/Performance_Test_Data_Storm_Arwen_PSALERTS.csv",
                // "-outputdir", "output",
                // "-compsOfInterest","AREC1/132_TX/T1A/TAPC",
            ],
        },
        {
            "name": "Run name server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-log", "output/name_server.log",
                "-loglevel", "warn",
                "-server",
                "-db", "../data_loads/load10/export_of_dataload10.db",
                
                // "-outputdir", "output",
                // "-compsOfInterest","AREC1/132_TX/T1A/TAPC",
            ],
        },
        {
            "name": "Read PSAlerts and resolve aliases from name service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-log", "output/alias_resolve_nameservice.log",
                "-loglevel", "warn",
                "-usenameservice",
                // "-db", "../data_loads/load10/export_of_dataload10.db",
                "-psalerts", "ref/Performance_Test_Data_Storm_Arwen_PSALERTS.csv",
                // "-outputdir", "output",
                // "-compsOfInterest","AREC1/132_TX/T1A/TAPC",
            ],
        },
    ]
}